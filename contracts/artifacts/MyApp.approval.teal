#pragma version 8
	b main

getSum:
	proto 2 1

	// contracts/my_app.algo.ts:6
	// return a + b;
	frame_dig -1 // a: uint64
	frame_dig -2 // b: uint64
	+
	retsub

getDifference:
	proto 2 1

	// contracts/my_app.algo.ts:10
	// return a >= b ? a - b : b - a;
	frame_dig -1 // a: uint64
	frame_dig -2 // b: uint64
	>=
	bz ternary0_false
	frame_dig -1 // a: uint64
	frame_dig -2 // b: uint64
	-
	b ternary0_end

ternary0_false:
	frame_dig -2 // b: uint64
	frame_dig -1 // a: uint64
	-

ternary0_end:
	retsub

abi_route_doMath:
	txn OnCompletion
	int NoOp
	==
	txn ApplicationID
	int 0
	!=
	&&
	assert
	byte 0x
	txna ApplicationArgs 3
	extract 2 0
	txna ApplicationArgs 2
	btoi
	txna ApplicationArgs 1
	btoi
	callsub doMath
	int 1
	return

doMath:
	proto 4 0

	// contracts/my_app.algo.ts:14
	// result: number
	// if0_condition
	// contracts/my_app.algo.ts:16
	// operation === 'sum'
	frame_dig -3 // operation: bytes
	byte "sum"
	==
	bz if0_elseif1_condition

	// if0_consequent
	// contracts/my_app.algo.ts:17
	// result = this.getSum(a, b)
	// no dupn needed
	frame_dig -2 // b: uint64
	frame_dig -1 // a: uint64
	callsub getSum
	frame_bury -4 // result: uint64
	b if0_end

if0_elseif1_condition:
	// contracts/my_app.algo.ts:18
	// operation === 'difference'
	frame_dig -3 // operation: bytes
	byte "difference"
	==
	bz if0_else

	// if0_elseif1_consequent
	// contracts/my_app.algo.ts:19
	// result = this.getDifference(a, b)
	// no dupn needed
	frame_dig -2 // b: uint64
	frame_dig -1 // a: uint64
	callsub getDifference
	frame_bury -4 // result: uint64
	b if0_end

if0_else:
	err // 'Invalid operation'

if0_end:
	// contracts/my_app.algo.ts:22
	// return result;
	frame_dig -4 // result: uint64
	itob
	byte 0x151f7c75
	swap
	concat
	log
	retsub

main:
	txn NumAppArgs
	bnz route_abi

	// default createApplication
	txn ApplicationID
	int 0
	==
	txn OnCompletion
	int NoOp
	==
	&&
	return

route_abi:
	method "doMath(uint64,uint64,string)uint64"
	txna ApplicationArgs 0
	match abi_route_doMath
	err